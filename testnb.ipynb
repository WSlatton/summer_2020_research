   "source": [
    "from poly import *\n",
    "\n",
    "R = PolyRing(['x', 'y', 'z'], field=RationalField, monomial_order=GrlexOrder)\n",
    "f = R('x^7y^2+x^3y^2-y+1')\n",
    "g1 = R('xy^2-x')\n",
    "g2 = R('x-y^3')\n",
    "\n",
    "qs, r = f/[g1,g2]\n",
    "qs[0] * g1 + qs[1] * g2 + r == f"
   ]
