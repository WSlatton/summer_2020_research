    "from poly import *\n",
    "\n",
    "R = PolyRing(['x'], field=RationalField)\n",
    "f = R('3x^3+2x')\n",
    "g = R('-2x+1')\n",
    "\n",
    "q, r = f/g\n",
    "q * g + r == f"
