   "source": [
    "from poly import *\n",
    "\n",
    "R = PolyRing(['x', 'y', 'z'], field=RationalField, monomial_order=GrevlexOrder)\n",
    "f = R('xy^2z^2+xy-yz')\n",
    "g1 = R('x-y^2')\n",
    "g2 = R('y-z^3')\n",
    "g3 = R('z^2-1')\n",
    "f/[g2,g3,g1]"
